{
  "author": "NetScout-Go",
  "description": "Performs DNS lookups for a domain",
  "icon": "dns",
  "id": "dns_lookup",
  "license": "MIT",
  "name": "DNS Lookup",
  "parameters": [
    {
      "default": "example.com",
      "description": "The domain name to lookup",
      "id": "domain",
      "name": "Domain",
      "required": true,
      "type": "string"
    },
    {
      "default": "A",
      "description": "Type of DNS record to lookup",
      "id": "recordType",
      "name": "Record Type",
      "options": [
        {
          "label": "A (IPv4 Address)",
          "value": "A"
        },
        {
          "label": "AAAA (IPv6 Address)",
          "value": "AAAA"
        },
        {
          "label": "MX (Mail Exchange)",
          "value": "MX"
        },
        {
          "label": "NS (Name Server)",
          "value": "NS"
        },
        {
          "label": "TXT (Text)",
          "value": "TXT"
        },
        {
          "label": "CNAME (Canonical Name)",
          "value": "CNAME"
        },
        {
          "label": "All Records",
          "value": "ALL"
        }
      ],
      "required": false,
      "type": "select"
    }
  ],
  "repository": "https://github.com/NetScout-Go/Plugin_dns_lookup",
  "version": "1.0.0"
}