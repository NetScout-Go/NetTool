{
  "id": "network_quality",
  "name": "Network Quality Monitor",
  "description": "Continuously monitors network quality metrics including jitter, packet loss, and latency",
  "icon": "timeline",
  "version": "1.0.0",
  "author": "NetScout-Go",
  "license": "MIT",
  "parameters": [
    {
      "id": "target",
      "name": "Target Host",
      "description": "The hostname or IP address to monitor",
      "type": "string",
      "required": true,
      "default": "8.8.8.8",
      "canIterate": true
    },
    {
      "id": "duration",
      "name": "Test Duration",
      "description": "Duration of the test in seconds",
      "type": "number",
      "required": false,
      "default": 30,
      "min": 10,
      "max": 300,
      "step": 10,
      "canIterate": true
    },
    {
      "id": "interval",
      "name": "Sampling Interval",
      "description": "Time between samples in seconds",
      "type": "number",
      "required": false,
      "default": 1,
      "min": 0.2,
      "max": 5,
      "step": 0.2,
      "canIterate": true
    },
    {
      "id": "packet_size",
      "name": "Packet Size",
      "description": "Size of the ICMP packets in bytes",
      "type": "number",
      "required": false,
      "default": 56,
      "min": 16,
      "max": 1472,
      "step": 8,
      "canIterate": true
    },
    {
      "id": "continueToIterate",
      "name": "Continue to iterate?",
      "description": "Enable repeated execution of this plugin with the same parameters",
      "type": "boolean",
      "required": false,
      "default": false,
      "canIterate": true
    },
    {
      "id": "iterationDelay",
      "name": "Iteration Delay (ms)",
      "description": "Delay between iterations in milliseconds",
      "type": "number",
      "required": false,
      "default": 5000,
      "min": 1000,
      "max": 60000,
      "step": 1000,
      "canIterate": true
    },
    {
      "id": "maxIterations",
      "name": "Max Iterations",
      "description": "Maximum number of iterations (0 = unlimited)",
      "type": "number",
      "required": false,
      "default": 0,
      "min": 0,
      "max": 100,
      "canIterate": true
    }
  ]
}
